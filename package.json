{
  "name": "json2proprietary-web-repl",
  "version": "0.0.0",
  "private": true,
  "main": "./bin/www",
  "scripts": {
    "postinstall": "npm link express-middleware-rollup lbs-connect-driver",
    "start": "forever start ./bin/www",
    "stop": "forever stop ./bin/www",
    "cluster-start": "forever start ./bin/www-cluster",
    "cluster-stop": "forever stop ./bin/www-cluster",
    "devstart": "nodemon --inspect",
    "babel-helpers": "babel-external-helpers -t global > public/javascripts/externals/babel-helpers.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stuartZhang/json2proprietary-web-repl.git"
  },
  "dependencies": {
    "argparse": "^1.0.9",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "fs-promise": "^2.0.3",
    "hbs": "~4.0.1",
    "morgan": "~1.9.0",
    "node-sass-middleware": "0.9.8",
    "postcss-middleware": "^1.1.4",
    "serve-favicon": "~2.4.5",
    "sticky-cluster": "^0.3.4",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "nodemon": "^1.12.1"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "delay": 2500,
    "verbose": true,
    "ext": "js json mjs bbjs",
    "ignore": [
      "node_modules/*",
      "public/*",
      "*.json",
      "*.md",
      "LICENSE"
    ],
    "env": {
      "NODE_ENV": "development",
      "DEBUG": "json2proprietary-web-repl:*"
    }
  }
}
