{
  "name": "json2proprietary-web-repl",
  "version": "0.0.0",
  "private": true,
  "main": "./bin/www",
  "scripts": {
    "start": "node ./bin/www",
    "devstart": "nodemon --inspect-brk"
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "hbs": "~4.0.1",
    "morgan": "~1.9.0",
    "node-sass-middleware": "0.9.8",
    "serve-favicon": "~2.4.5"
  },
  "devDependencies": {
    "nodemon": "^1.12.1"
  },
  "nodemonConfig": { // reference: https://github.com/remy/nodemon/blob/master/doc/sample-nodemon.md
    "restartable": "rs",
    "delay": 2500,
    "verbose": true,
    "ext": "js json mjs bbjs",
    "ignore": [
      "node_modules/*",
      "public/*",
      "*.json",
      "*.md",
      "LICENSE"
    ],
    "env": {
      "NODE_ENV": "development",
      "DEBUG": "json2proprietary-web-repl:*"
    }
  }
}
